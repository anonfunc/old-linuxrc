Xcursor.theme: whiteglass
XTerm.faceSize: 16
!Xft.dpi: 96
urxvt.perl-lib: /home/first/.urxvt/urxvt-font
URxvt.perl-ext-common: default,matcher,searchable-scrollback,selection-autotransform,font
urxvt.keysym.Control-Meta-Up:      perl:font:increment
urxvt.keysym.Control-Meta-Down:    perl:font:decrement
URxvt.matcher.button: 1
URxvt.matcher.pattern.1:  \\bwww\\.[\\w-]+\\.[\\w./?&@#-]*[\\w/-]
URxvt.matcher.pattern.2: (CR-[A-Z]{3,7}-[0-9]+)
URxvt.matcher.launcher.2: sensible-browser https\:\057\057crucible.workplace.com/cru/$1
URxvt.matcher.pattern.3: (?<!CR-)([omsdplyA-Z]{3,7}-[0-9]+)
URxvt.matcher.launcher.3: sensible-browser https\:\057\057jira.workplace.com/browse/$1
!URxvt.matcher.pattern.4: (?<!CR-)([A-Z][A-Z][A-Z][A-Z]?[A-Z]?-[1-9]+)
!URxvt.matcher.launcher.4: sensible-browser https\:\057\057jira.workplace.com/browse/$1
URxvt.matcher.pattern.4:  \\B/.*\\.html

URxvt.selection.pattern-0: ([0-9.]+\\/[oA-Z][mA-Z][sA-Z]{0,3}-[0-9]+)
URxvt.selection-autotransform.0: s/^([0-9.]+\\/[oA-Z][mA-Z][sA-Z]{0,3}-[0-9]+)$/git checkout $1/
URxvt.selection-autotransform.1: s/^([a-z0-9]{40}|[a-z0-9]{7})$/git show $1/
URxvt.selection-autotransform.2: s/^(.*\.dump)$/less $1/
URxvt.selection-autotransform.3: s/^[ab]\\/(.*)$/e `gw $1`/

URxvt.scrollBar: 0
!URxvt.background: #222222
!URxvt.foreground: #fafafa
URxvt.visualBell: true
URxvt.urgentOnBell: true
URxvt.font:             xft:Envy Code R:autohint=true:size=16
URxvt.italicFont:       xft:Envy Code R:italic:autohint=true:size=16
URxvt.boldFont:         xft:Envy Code R:bold:autohint=true:size=16
URxvt.boldItalicFont:   xft:Envy Code R:bold:italic:autohint=true:size=16
! URxvt.font:             xft:Inconsolata-BoldPunct:autohint=true:size=16
! URxvt.italicFont:       xft:Inconsolata-BoldPunct:italic:autohint=true:size=16
! URxvt.boldFont:         xft:Inconsolata-BoldPunct:bold:autohint=true:size=16
! URxvt.boldItalicFont:   xft:Inconsolata-BoldPunct:bold:italic:autohint=true:size=16
URxvt.pointerBlank: true

! Tweaks for home/end, del.
!
!URxvt.font:             9x15bold,\
!                        -misc-fixed-bold-r-normal--15-140-75-75-c-90-iso10646-1,\
!                        -misc-fixed-medium-r-normal--15-140-75-75-c-90-iso10646-1, \
!                        xft:Bitstream Vera Sans Mono:autohint=true
!URxvt.boldFont:         -xos4-terminus-bold-r-normal--14-140-72-72-c-80-iso8859-15
!URxvt.italicFont:       xft:Bitstream Vera Sans Mono:italic:autohint=true
!URxvt.boldItalicFont:   xft:Bitstream Vera Sans Mono:bold:italic:autohint=true

Emacs.FontBackend: xft
!Emacs.FontBackend: x
!Emacs.Font: Monospace-12
!Emacs.Font: Inconsolata-14
!Emacs.Font: Inconsolata-16:antialias=True
Emacs.Font: Consolas:antialias=True:size=20

